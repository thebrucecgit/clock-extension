{
  "manifest_version": 2,
  "name": "College Clock Extension",
  "description": "A neat little clock for Christ's College Schoolbox. ",
  "version": "0.8",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "clock.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://schoolbox.christscollege.com/*"
      ],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ]
}